___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const copyFromWindow = require('copyFromWindow');
const createArgumentsQueue = require('createArgumentsQueue');

const gtag = createArgumentsQueue('gtag', 'dataLayer');
const hj = copyFromWindow('hj');

function hjEvents(value, name) {
  if (name == data.optimizeExperimentID) {
    if (value ==  '0') {
      hj('event', data.eventNameControlGroup);
    } else if (data.hotjarEvents[value-1].eventNameVariant) {
      hj('event', data.hotjarEvents[value-1].eventNameVariant);
    }
  }
}

gtag('event', 'optimize.callback', {
  callback: hjEvents
});

data.gtmOnSuccess();


